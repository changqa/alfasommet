var sub = function (val) { if (val.length == 0 || val == undefined) { return; } if (val.length > 17) { return val.substring(0, 17) + "..."; } else { return val; } } 
module.exports.sub = sub;

var getIndex = function(val) {
  var index = charAt(val.length-1);
  return index;
}

module.exports.getIndex = getIndex;